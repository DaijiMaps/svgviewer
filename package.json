{
  "name": "svgviewer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host 0.0.0.0",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --workspace=vitest.workspace.ts --project unit",
    "test:unit": "vitest --workspace=vitest.workspace.ts --project unit",
    "test:jsdom": "vitest --workspace=vitest.workspace.ts --project jsdom",
    "test:browser": "vitest --workspace=vitest.workspace.ts --project browser",
    "pages:deploy": "pnpm build && cd dist && wrangler pages deploy ."
  },
  "dependencies": {
    "@xstate/react": "^4.1.2",
    "effect": "^3.7.2",
    "flatbush": "^4.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "transformation-matrix": "^2.16.1",
    "two.js": "^0.8.14",
    "type-fest": "^4.26.1",
    "usehooks-ts": "^3.1.0",
    "xstate": "^5.18.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/browser": "^2.0.5",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-functional": "^7.0.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "5.1.0-rc-fb9a90fa48-20240614",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^25.0.0",
    "playwright": "^1.47.0",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.5.0",
    "vite": "^5.4.3",
    "vitest": "^2.0.5"
  }
}
